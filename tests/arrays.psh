let a = [0, 1, 2];
let removed = a.remove(0);
assert(removed == 0);
assert(a.len == 2);
assert(a[0] == 1);
assert(a[1] == 2);

let a2 = [0, 1, 2];
a2.insert(1, 55);
assert(a2.len == 4);
assert(a2[0] == 0);
assert(a2[1] == 55);
assert(a2[2] == 1);

let a3 = [1, 2];
let b3 = [3, 4];
a3.append(b3);
assert(a3.len == 4);
assert(a3[0] == 1);
assert(a3[1] == 2);
assert(a3[2] == 3);
assert(a3[3] == 4);
assert(b3.len == 2);

let c3 = [1, 2];
c3.append(c3);
assert(c3.len == 4);
assert(c3[0] == 1);
assert(c3[1] == 2);
assert(c3[2] == 1);
assert(c3[3] == 2);

$println("Array tests passed.");
