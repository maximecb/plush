class Node
{
    init(self, val, next)
    {
        self.val = val;
        self.next = next;
    }
}

let var list = nil;

for (let var n = 0; n < 2_000; ++n)
{
    list = nil;

    for (let var i = 0; i < 4_000; ++i)
    {
        list = Node(1, list);
    }

    let var sum = 0;

    for (let var node = list; node != nil; node = node.next)
    {
        sum = sum + node.val;
    }

    assert(sum == 4_000);
}
