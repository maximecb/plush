class Foo
{
    init(self)
    {
        self.a = 1;
        self.b = 2;
    }

    m1(self)
    {
        return 'foo1';
    }

    m2(self)
    {
        return 'foo2';
    }
}

class Bar extends Foo
{
    init(self)
    {
        Foo.init(self);
        self.b = 77;
        self.c = 4;
    }

    m2(self)
    {
        return 'bar';
    }
}

let f = Foo();
assert(f instanceof Foo);
assert(f.a == 1);
assert(f.b == 2);

let b = Bar();
assert(b instanceof Bar);
assert(b.a == 1);
assert(b.b == 77);
assert(b.c == 4);
assert(b.m1() == 'foo1');
assert(b.m2() == 'bar');
