let bytes = $read_file('README.md');
assert(bytes.len > 100);

// First heading at the top of the file
assert(String.from_codepoint(bytes[0]) == '#');

$println('File length: ' + bytes.len.to_s());

let s = $read_file_utf8('README.md');
assert(s.len > 100);
//assert(s.char_at(0) == '#');

$println('String length: ' + s.len.to_s());
