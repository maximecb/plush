class Node
{
    init(self, val, next)
    {
        self.val = val;
        self.next = next;
    }
}

let var list = nil;
let num_nodes = 1_000_000;

// Grow the list, which will cause multiple GC cycles
for (let var i = 0; i < num_nodes; ++i)
{
    list = Node(1, list);
}

// Trigger multiple GC cycles in a loop
for (let var i = 0; i < 10_000; ++i)
{
    ByteArray.with_size(100_000);
}
